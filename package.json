{
    "name": "@nexcodepl/aws-cognito-auth-server",
    "version": "1.0.0",
    "description": "Middleware for AWS Cognito authentication",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": "https://github.com/NexCodePL/aws-cognito-auth-server.git",
    "author": "Alexander Stefa≈Ñski <alex@nexcode.pl>",
    "license": "MIT",
    "keywords": [
        "nexcodepl",
        "aws",
        "cognito",
        "server"
    ],
    "private": false,
    "scripts": {
        "build": "yarn tsc",
        "format": "yarn prettier --write \"src/**/*.ts\"",
        "lint": "yarn eslint . --ext .ts",
        "prepare": "yarn build",
        "prepublishOnly": "yarn lint",
        "preversion": "yarn lint",
        "version": "yarn format && git add -A src",
        "postversion": "git push && git push --tags",
        "publish:npm": "npm publish --access public"
    },
    "dependencies": {
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.4"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^8.5.0",
        "@types/jwk-to-pem": "^2.0.0",
        "@types/node": "^14.14.3",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "eslint": "^7.12.0",
        "prettier": "^2.1.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    },
    "files": [
        "lib/**/*"
    ]
}
